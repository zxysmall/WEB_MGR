<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.PayFlowMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.PayFlow">
        <id column="seq" property="seq" />
        <result column="status" property="status" />
        <result column="order_id" property="orderId" />
        <result column="excuted_class" property="excutedClass" />
        <result column="processId" property="processId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        seq, status, order_id AS orderId, excuted_class AS excutedClass, processId
    </sql>

</mapper>
